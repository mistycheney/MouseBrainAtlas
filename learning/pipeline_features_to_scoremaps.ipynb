    "# for stack in all_stacks:\n",
    "for stack in ['MD635']:\n",
    "        \n",
    "    first_sec, last_sec = metadata_cache['section_limits'][stack]\n",
    "\n",
    "#     #################################\n",
    "\n",
    "    t = time.time()\n",
